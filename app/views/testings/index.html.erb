<p id="notice"><%= notice %></p>

<h1>Listing Testings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @testings.each do |testing| %>
      <tr>
        <td><%= link_to 'Show', testing %></td>
        <td><%= link_to 'Edit', edit_testing_path(testing) %></td>
        <td><%= link_to 'Destroy', testing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Testing', new_testing_path %>
